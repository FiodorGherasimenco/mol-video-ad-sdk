{
  "name": "@mailonline/video-ad-sdk",
  "version": "0.3.0",
  "description": "Video ad SDK to load and play video ads.",
  "author": "MailOnline <fe@mailonline.co.uk>",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production rollup -c",
    "dev": "rollup -c",
    "docs": "rimraf docs && jsdoc -c jsdoc.json",
    "lint": "eslint --ignore-path ./.gitignore './**/*.js'",
    "test": "cross-env NODE_ENV=test jest",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "precommit": "npm run lint",
    "prepush": "npm test",
    "publish-npm": "npm publish"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MailOnline/@mailonline/video-ad-sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/MailOnline/@mailonline/video-ad-sdk.git"
  },
  "homepage": "https://github.com/MailOnline/@mailonline/video-ad-sdk#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/mol-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "./node_modules/mol-commitlint-config"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/travis-cli": "^7.0.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.9.0",
    "eslint-config-mailonline": "^9.0.0",
    "husky": "^1.1.2",
    "jsdoc": "^3.5.5",
    "mol-commitlint-config": "^1.1.0",
    "mol-conventional-changelog": "^1.2.0",
    "rollup": "^0.63.5",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.4",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-terser": "^1.0.1",
    "tui-jsdoc-template": "^1.2.2"
  }
}
